---
layout: null
---
[
  {% assign pages = site.pages | where_exp: "item",
    "item.title" %
  }
  {% for page in pages %
  }
    {% if page.layout == 'default' %
  }
      {
    "title": "{{ page.title | escape }}",
    "url": "{{ site.baseurl }}{{ page.url }}",
    "content": "{{ page.content | strip_html | strip_newlines | escape }}"
  }{% unless forloop.last %
  },
  {% endunless %
  }
    {% endif %
  }
  {% endfor %
  }
]